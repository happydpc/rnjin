    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.obj": true,
        "**/*.exe": true,
        "**/.sconsign.dblite": true,
        "LICENSE": true,
        "README.md": true,
    },
    "highlight.regexes": {
        "(\\btext)(\\()(.+?),\\s+(.+?)(\\))" : {
            "decorations": [
                { "color": "#e06c75" },
                { },
                { "color": "#56b6c2" },
                { },
            ]
        },
        "(get_text)(\\()(.+?)(\\))" : {
            "decorations": [
                { "color": "#e5c07b" },
                { },
                { "color": "#56b6c2" },
                { },
            ]
        },
        "(\\bstring\\b)": {
            "color": "#56b6c2"
        },
        "(\\b(and|or|xor|not)\\b)": {
            "color": "#e06c75"
        },
        "(\\b(nonconst)\\b)": {
            "color": "#e06c75"
        },
        "(\\btypename\\b)": {
            "color": "#e5c07b"
        },
        "(\\blist\\b)": {
            "color": "#56b6c2"
        },
        "(\\bdictionary\\b)": {
            "color": "#56b6c2"
        },
        "([^:\\s<\\(\\[\\.&\\*!~]*)(::)": {
            "decorations": [
                { "color": "#56b6c2", "textDecoration": "underline" },
                { "color": "#e06c75" },
            ]
        },
        "(\\[)([^\\]]*?)(\\])": {
            "decorations": [
                { "color": "#e06c75" },
                {  },
                { "color": "#e06c75" },
            ]
        },
    }
}